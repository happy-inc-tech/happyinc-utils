{
  "name": "happyinc-utils",
  "version": "1.1.0",
  "description": "Helper functions we use at Happy Inc",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./packages",
    "test": "jest",
    "lint": "eslint './src/**/*.ts' --quiet --fix",
